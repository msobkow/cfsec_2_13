delimiter ////
create procedure cfsec213.sp_delete_svctype_by_ididx(
	argAuditClusterId bigint,
	argAuditUserId varchar(36),
	argAuditSessionId varchar(36),
	secClusterId bigint,
	secTenantId bigint,
	argServiceTypeId integer )
not deterministic
modifies sql data
begin
	declare curRevision int;
	declare subret boolean;
	declare curServiceTypeId integer;
	declare done int default false;
	declare cur_ididx cursor for
		select
			a013.servicetypeid as servicetypeid,
			a013.revision as revision
		from cfsec213.SvcType as a013
		where
			a013.servicetypeid = argServiceTypeId;
	declare cur_ididx_normal cursor for
		select
			a013.servicetypeid as servicetypeid,
				a013.revision as revision
		from cfsec213.SvcType as a013
		where
				a013.servicetypeid = argServiceTypeId;
	declare continue handler for not found set done = true;

	open cur_ididx_normal;
	read_loop_ididx_normal: loop
		fetch cur_ididx_normal into
				curServiceTypeId,
				curRevision;
		if done then
			leave read_loop_ididx_normal;
		end if;
			call cfsec213.sp_delete_svctype( argAuditClusterId,
						argAuditUserId,
						argAuditSessionId,
						secClusterId,
						secTenantId,
						curServiceTypeId,
						curRevision );
	end loop;
	close cur_ididx_normal;
end;////
