delimiter ////
create procedure cfsec213.sp_delete_iso_cntry_by_nameidx(
	argAuditClusterId bigint,
	argAuditUserId varchar(36),
	argAuditSessionId varchar(36),
	secClusterId bigint,
	secTenantId bigint,
	argName varchar(64) )
not deterministic
modifies sql data
begin
	declare curRevision int;
	declare subret boolean;
	declare curISOCtryId smallint;
	declare done int default false;
	declare cur_nameidx cursor for
		select
			a004.isoctryid as isoctryid,
			a004.revision as revision
		from cfsec213.iso_cntry as a004
		where
			a004.country_name = argName;
	declare cur_nameidx_normal cursor for
		select
			a004.isoctryid as isoctryid,
				a004.revision as revision
		from cfsec213.iso_cntry as a004
		where
				a004.country_name = argName;
	declare continue handler for not found set done = true;

	open cur_nameidx_normal;
	read_loop_nameidx_normal: loop
		fetch cur_nameidx_normal into
				curISOCtryId,
				curRevision;
		if done then
			leave read_loop_nameidx_normal;
		end if;
			call cfsec213.sp_delete_iso_cntry( argAuditClusterId,
						argAuditUserId,
						argAuditSessionId,
						secClusterId,
						secTenantId,
						curISOCtryId,
						curRevision );
	end loop;
	close cur_nameidx_normal;
end;////
