delimiter ////
create procedure cfsec213.sp_delete_iso_cntrylng_by_ctryidx(
	argAuditClusterId bigint,
	argAuditUserId varchar(36),
	argAuditSessionId varchar(36),
	secClusterId bigint,
	secTenantId bigint,
	argISOCtryId smallint )
not deterministic
modifies sql data
begin
	declare curRevision int;
	declare subret boolean;
	declare curISOCtryId smallint;
	declare curISOLangId smallint;
	declare done int default false;
	declare cur_ctryidx cursor for
		select
			a006.isoctryid as isoctryid,
			a006.isolangid as isolangid,
			a006.revision as revision
		from cfsec213.iso_cntrylng as a006
		where
			a006.isoctryid = argISOCtryId;
	declare cur_ctryidx_normal cursor for
		select
			a006.isoctryid as isoctryid,
			a006.isolangid as isolangid,
				a006.revision as revision
		from cfsec213.iso_cntrylng as a006
		where
				a006.isoctryid = argISOCtryId;
	declare continue handler for not found set done = true;

	open cur_ctryidx_normal;
	read_loop_ctryidx_normal: loop
		fetch cur_ctryidx_normal into
				curISOCtryId,
				curISOLangId,
				curRevision;
		if done then
			leave read_loop_ctryidx_normal;
		end if;
			call cfsec213.sp_delete_iso_cntrylng( argAuditClusterId,
						argAuditUserId,
						argAuditSessionId,
						secClusterId,
						secTenantId,
						curISOCtryId,
						curISOLangId,
						curRevision );
	end loop;
	close cur_ctryidx_normal;
end;////
