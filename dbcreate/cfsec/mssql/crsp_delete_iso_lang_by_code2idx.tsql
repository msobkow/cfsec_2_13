use cfsec213
go

create proc sp_delete_iso_lang_by_code2idx(
	@argAuditClusterId bigint,
	@argAuditUserId varchar(36),
	@argAuditSessionId varchar(36),
	@secClusterId bigint,
	@secTenantId bigint,
	@argISO6391Code varchar(2) )
as
	declare @varISOLangId CFSec213ISOLangIdGenSeq
	declare @varRevision int
	declare @retval int
	declare cursNormaliso_langCode2Idx cursor local for
		select
			a007.isolangid as isolangid,
			a007.revision as revision
		from cfsec213..iso_lang as a007
		where
			( ( ( @argISO6391Code is null ) and ( a007.iso_code2 is null ) )
				or ( ( @argISO6391Code is not null ) and ( a007.iso_code2 = @argISO6391Code ) ) )
begin
	open cursNormaliso_langCode2Idx
	fetch next from cursNormaliso_langCode2Idx into
				@varISOLangId,
				@varRevision
	while ( @@fetch_status = 0 )
	begin
				exec sp_delete_iso_lang @argAuditClusterId,
						@argAuditUserId,
						@argAuditSessionId,
						@secClusterId,
						@secTenantId,
						@varISOLangId,
						@varRevision
		fetch next from cursNormaliso_langCode2Idx into
				@varISOLangId,
				@varRevision
	end
	close cursNormaliso_langCode2Idx
	deallocate cursNormaliso_langCode2Idx
end
go

quit
