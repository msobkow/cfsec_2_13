use cfsec213
go

create proc sp_delete_iso_cntryccy_by_ctryidx(
	@argAuditClusterId bigint,
	@argAuditUserId varchar(36),
	@argAuditSessionId varchar(36),
	@secClusterId bigint,
	@secTenantId bigint,
	@argISOCtryId smallint )
as
	declare @varISOCtryId CFSec213ISOCtryIdGenSeq
	declare @varISOCcyId CFSec213ISOCcyIdGenSeq
	declare @varRevision int
	declare @retval int
	declare cursNormaliso_cntryccyCtryIdx cursor local for
		select
			a005.isoctryid as isoctryid,
			a005.isoccyid as isoccyid,
			a005.revision as revision
		from cfsec213..iso_cntryccy as a005
		where
			a005.isoctryid = @argISOCtryId
begin
	open cursNormaliso_cntryccyCtryIdx
	fetch next from cursNormaliso_cntryccyCtryIdx into
				@varISOCtryId,
				@varISOCcyId,
				@varRevision
	while ( @@fetch_status = 0 )
	begin
				exec sp_delete_iso_cntryccy @argAuditClusterId,
						@argAuditUserId,
						@argAuditSessionId,
						@secClusterId,
						@secTenantId,
						@varISOCtryId,
						@varISOCcyId,
						@varRevision
		fetch next from cursNormaliso_cntryccyCtryIdx into
				@varISOCtryId,
				@varISOCcyId,
				@varRevision
	end
	close cursNormaliso_cntryccyCtryIdx
	deallocate cursNormaliso_cntryccyCtryIdx
end
go

quit
