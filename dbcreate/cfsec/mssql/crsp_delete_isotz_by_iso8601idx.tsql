use cfsec213
go

create proc sp_delete_isotz_by_iso8601idx(
	@argAuditClusterId bigint,
	@argAuditUserId varchar(36),
	@argAuditSessionId varchar(36),
	@secClusterId bigint,
	@secTenantId bigint,
	@argIso8601 varchar(6) )
as
	declare @varISOTZoneId CFSec213ISOTZoneIdGenSeq
	declare @varRevision int
	declare @retval int
	declare cursNormalISOTzIso8601Idx cursor local for
		select
			a008.isotzoneid as isotzoneid,
			a008.revision as revision
		from cfsec213..ISOTz as a008
		where
			a008.iso8601 = @argIso8601
begin
	open cursNormalISOTzIso8601Idx
	fetch next from cursNormalISOTzIso8601Idx into
				@varISOTZoneId,
				@varRevision
	while ( @@fetch_status = 0 )
	begin
				exec sp_delete_isotz @argAuditClusterId,
						@argAuditUserId,
						@argAuditSessionId,
						@secClusterId,
						@secTenantId,
						@varISOTZoneId,
						@varRevision
		fetch next from cursNormalISOTzIso8601Idx into
				@varISOTZoneId,
				@varRevision
	end
	close cursNormalISOTzIso8601Idx
	deallocate cursNormalISOTzIso8601Idx
end
go

quit
