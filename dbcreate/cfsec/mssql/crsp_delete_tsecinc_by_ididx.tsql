use cfsec213
go

create proc sp_delete_tsecinc_by_ididx(
	@argAuditClusterId bigint,
	@argAuditUserId varchar(36),
	@argAuditSessionId varchar(36),
	@secClusterId bigint,
	@secTenantId bigint,
	@argTenantId decimal(21,0),
	@argTSecGrpIncId decimal(21,0) )
as
	declare @varTenantId CFSec213TenantIdGenSeq
	declare @varTSecGrpIncId CFSec213TSecGrpIncIdGen
	declare @varRevision int
	declare @retval int
	declare cursNormalTSecIncIdIdx cursor local for
		select
			a017.tenantid as tenantid,
			a017.tsecgrpincid as tsecgrpincid,
			a017.revision as revision
		from cfsec213..TSecInc as a017
		where
			a017.tenantid = @argTenantId
			and a017.tsecgrpincid = @argTSecGrpIncId
	declare cursRestrictTSecIncIdIdx cursor local for
		select
			a017.tenantid as tenantid,
			a017.tsecgrpincid as tsecgrpincid,
			a017.revision as revision
		from cfsec213..TSecInc as a017
		where
			a017.tenantid = @argTenantId
			and a017.tsecgrpincid = @argTSecGrpIncId
			and a017.tenantid = @secTenantId
begin
	exec sp_is_system_user @retval output, @argAuditUserId
	if( @retval = 1 )
	begin
		open cursNormalTSecIncIdIdx
		fetch next from cursNormalTSecIncIdIdx into
				@varTenantId,
				@varTSecGrpIncId,
				@varRevision
		while ( @@fetch_status = 0 )
		begin
				exec sp_delete_tsecinc @argAuditClusterId,
						@argAuditUserId,
						@argAuditSessionId,
						@secClusterId,
						@secTenantId,
						@varTenantId,
						@varTSecGrpIncId,
						@varRevision
			fetch next from cursNormalTSecIncIdIdx into
				@varTenantId,
				@varTSecGrpIncId,
				@varRevision
		end
		close cursNormalTSecIncIdIdx
		deallocate cursNormalTSecIncIdIdx
	end
	else begin
		open cursRestrictTSecIncIdIdx
		fetch next from cursRestrictTSecIncIdIdx into
				@varTenantId,
				@varTSecGrpIncId,
				@varRevision
		while ( @@fetch_status = 0 )
		begin
				exec sp_delete_tsecinc @argAuditClusterId,
						@argAuditUserId,
						@argAuditSessionId,
						@secClusterId,
						@secTenantId,
						@varTenantId,
						@varTSecGrpIncId,
						@varRevision
			fetch next from cursRestrictTSecIncIdIdx into
				@varTenantId,
				@varTSecGrpIncId,
				@varRevision
		end
		close cursRestrictTSecIncIdIdx
		deallocate cursRestrictTSecIncIdIdx
	end
end
go

quit
